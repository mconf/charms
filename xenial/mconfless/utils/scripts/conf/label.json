{
  "audio": {
    "dialog": {
      "microphone": "[aria-label=\"Microphone\"]",
      "listen": "[aria-label=\"Listen Only\"]",
      "close": "[aria-label=\"Close\"]"
    },
    "echo": {
      "confirm": "[aria-label=\"Echo is audible\"]"
    },
    "join": "[aria-label=\"Join Audio\"]",
    "mute": "[aria-label=\"Mute\"]",
    "unmute": "[aria-label=\"Unmute\"]",
    "leave": "[aria-label=\"Leave Audio\"]"
  },
  "video": {
    "open": "[aria-label=\"Open video menu dropdown\"]",
    "menu": {
      "share": "img[src=\"/html5client/resources/images/video-menu/icon-webcam-off.svg\"]",
      "unshare": "img[src=\"/html5client/resources/images/video-menu/icon-webcam-off.svg\"]",
      "swap": "img[src=\"/html5client/resources/images/video-menu/icon-swap.svg\"]"
    },
    "settings": {
      "start": "[aria-label=\"Start Sharing\"]"
    },
    "kurento": "a[id=\"viewer\"]"
  },
  "chat": {
    "open": "a[href=\"/html5client/users/chat/public\"]",
    "close": "[aria-label=\"Hide Public Chat\"]",
    "form": {
      "input": "#message-input",
      "send": "[aria-label=\"Send Message\"]"
    }
  },
  "note": {
    "open": "a[href=\"/html5client/users/notes\"]",
    "close": "[aria-label=\"Hide Shared Notes\"]",
    "frame": {
      "name": "ace_inner",
      "pad": "html[dir=\"ltr\"]"
    }
  }
}
